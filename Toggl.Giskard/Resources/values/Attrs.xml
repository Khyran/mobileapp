<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextInputLayoutWithHelperText">
        <attr name="helperText" format="string" />
        <attr name="helperTextColor" format="color" />
        <attr name="helperTextSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="WheelDurationInput">
        <attr name="fadedTextColor" format="color" />
    </declare-styleable>
    <declare-styleable name="PomodoroWorkflowView">
        <attr name="selectionMode" format="enum">
            <enum name="none" value="0"/>
            <enum name="manual" value="1"/>
            <enum name="userInteraction" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="PomodoroDurationSeekbar">
        <attr name="allowedDurations" format="string" />
    </declare-styleable>
</resources>